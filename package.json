{
  "name": "curity-maven-gh-action",
  "version": "1.0.0",
  "description": "GitHub Action to setup Maven with OAuth access token",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.js --source-map --license licenses.txt",
    "package": "npm run build && git add dist",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/maven-oauth-action.git"
  },
  "keywords": [
    "actions",
    "maven",
    "oauth",
    "authentication"
  ],
  "author": "Renato Athaydes",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/io": "^1.1.3",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0"
  },
  "engines": {
    "node": ">=20"
  }
}